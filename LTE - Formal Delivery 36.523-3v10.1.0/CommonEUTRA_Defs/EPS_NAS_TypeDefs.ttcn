  B3_Type                     typeValue,
  octetstring                 adressInfo length(4..12)
};


type record TrackingAreaId {                                  /* 24.301 cl. 9.9.3.32 */
  IEI8_Type                   iei                   optional, /* present in case of TV; omit in case of V */
