};

type record TrackingAreaIdList {                              /* 24.301 cl. 9.9.3.33 */
  IEI8_Type                   iei                   optional, /* present in case of TLV; omit in case of LV */
  Type4Length_Type            iel,
  record length (1..16) of ListOfPartialTaiList  listOfPartialTais
};

