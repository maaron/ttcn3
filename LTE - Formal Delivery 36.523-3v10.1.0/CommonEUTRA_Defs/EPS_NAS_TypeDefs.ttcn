};

type record Type0Element {  // @sic R5s100184 sic@
  NAS_PlmnId                  plmnId,                         /* MCC  + MNC 3 digits each */
  NAS_TacList                 tacList
};

type record Type1Element {  // @sic R5s100184 sic@
  PartialTai                  partialTac
};

type record length (1..16) of PartialTai PlmnAndTacs;

type record Type2Element {  // @sic R5s100184 sic@
  PlmnAndTacs    plmnAndTacs
};

type record length (1..16) of Type0Element Type0List;
type record length (1..16) of Type1Element Type1List;
type record length (1..16) of Type2Element Type2List;

type union PartialTaiType {
  Type0List   type0List,
  Type1List   type1List,
  Type2List   type2List
};

type record ListOfPartialTaiList {
  B1_Type                  spareBit,
  EPS_PartialTaiListType   typeOfList,
  B5_Type                  numberOfElements,
